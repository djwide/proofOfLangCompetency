Cell["The System", "Subsubsection"],

Cell["Here, we find any fixed points:", "Text"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f1", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"y", "^", "2"}], "-", "1", " ", "-", " ", "x"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f2", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"x", "^", "2"}], "-", "1", " ", "-", " ", "y"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"f1", "[", 
        RowBox[{"x", ",", "y"}], "]"}], "\[Equal]", "0"}], ",", 
      RowBox[{
